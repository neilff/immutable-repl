{
  "name": "immutable-repl",
  "version": "1.0.0",
  "description": "A slim React & Redux starter project for quickly prototyping ideas",
  "engines": {
    "node": "4.x"
  },
  "scripts": {
    "postinstall": "npm run clean; npm run build;",
    "build": "NODE_ENV=production webpack -p",
    "clean": "rimraf dist",
    "cover": "istanbul cover _mocha -- --opts ./mocha.opts src/**/*.test.js",
    "dev": "node server.js",
    "lint": "eslint .",
    "test": "npm run lint && mocha --opts ./mocha.opts src/**/*.test.js",
    "test:watch": "npm run test -- --watch",
    "start": "NODE_ENV=production node server.js"
  },
  "keywords": [
    "react",
    "redux"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/neilff/immutable-repl.git"
  },
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.4.0",
    "istanbul": "^0.4.0",
    "jsdom": "^7.0.2",
    "mocha": "^2.3.3",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "^1.12.1",
    "webpack-hot-middleware": "^2.5.0"
  },
  "dependencies": {
    "babel": "^5.5.8",
    "babel-core": "^5.6.18",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.1.4",
    "base-64": "^0.1.0",
    "basscss": "^7.0.4",
    "css-loader": "^0.23.0",
    "cssnext-loader": "^1.0.1",
    "debounce": "^1.0.0",
    "eslint": "^1.8.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.6.3",
    "express": "^4.13.3",
    "file-loader": "^0.8.4",
    "history": "^1.13.0",
    "immutable": "^3.7.5",
    "json-loader": "^0.5.3",
    "json-stringify-pretty-compact": "^1.0.1",
    "postcss-loader": "^0.8.0",
    "query-string": "^3.0.0",
    "react": "^0.14.2",
    "react-codemirror": "^0.2.2",
    "react-dom": "^0.14.2",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4",
    "redux-localstorage": "^0.4.0",
    "redux-logger": "^2.0.4",
    "redux-thunk": "^1.0.0",
    "rimraf": "^2.4.3",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "winston": "^2.1.1"
  }
}
